{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "useDefineForClassFields": true,
        "moduleResolution": "bundler",
        "types": [
            "vite/client"
        ],
        "declaration": false
    }
}
